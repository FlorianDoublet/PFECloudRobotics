<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="turtlebot" scheme="http://occiware.org/turtlebot#">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <kinds term="turtlebot" scheme="http://occiware.org/turtlebot#">
    <attributes name="max_speeds" default="" description="Speeds of the linear speed and the angular speed&#xD;&#xA;">
      <type href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="linear_speed" description="The linear speed of the turtlebot">
      <type href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="angular_speed" description="Angular speed of the turtlebot">
      <type href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="duration" default="1000" description="Duration of a moving commande (forward or backward)">
      <type href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <parent href="http://schemas.ogf.org/occi/core#//@kinds[term='resource']"/>
    <actions term="move_forward" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="The turtlebot move forward"/>
    <actions term="move_backward" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="The turtlebot move backward"/>
    <actions term="move_left" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="The turtlebot turn left"/>
    <actions term="move_right" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="The turtlebot turn right"/>
    <actions term="increase_linear_speed" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="Increase the linear speed by 10%"/>
    <actions term="decrease_linear_speed" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="Decrease the linear speed by 10%"/>
    <actions term="inrease_angular_speed" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="increase the angular speed by 10%"/>
    <actions term="decrease_angular_speed" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="Decrease the angular speed by 10%"/>
    <actions term="stop" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="Force the turtlebot to stop"/>
    <actions term="stop_smoothly" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="Stop the turtlebot smoothly"/>
  </kinds>
</occi:Extension>
