<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="turtlebot" scheme="http://occiware.org/turtlebot#">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <kinds term="turtlebot" scheme="http://occiware.org/turtlebot#">
    <attributes name="duration" default="1000" description="Duration of a moving commande (forward or backward)">
      <type href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="user">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="ip_address">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="password">
      <type href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <parent href="http://schemas.ogf.org/occi/core#//@kinds[term='resource']"/>
    <actions term="move_forward" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="The turtlebot move forward"/>
    <actions term="move_backward" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="The turtlebot move backward"/>
    <actions term="turn_left" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="The turtlebot turn left"/>
    <actions term="turn_right" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="The turtlebot turn right"/>
    <actions term="stop" scheme="http://occiware.org/turtlebot/turtlebot/action#" title="Force the turtlebot to stop"/>
  </kinds>
</occi:Extension>
